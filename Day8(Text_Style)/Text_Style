    return MaterialApp(
      home: Scaffold(
          appBar: AppBar(
            title: Text('Sample app'),
          ),
          body: const Center(
            child: Text(
              'Default App',
              style: TextStyle(fontSize: 25),
            ),
          )),
    );
  }
}
